{
  "$schema": "https://turbo.build/schema.json",
  "pipeline": {
    "build": {
      "dependsOn": ["^build"],
      "outputs": ["dist/**"]
    },
    "@midi-editor/react#build": {
      "dependsOn": ["^build"],
      "outputs": ["dist/**"]
    },
    "@midi-editor/app#build": {
      "dependsOn": ["^build", "^@midi-editor/react#build"],
      "outputs": [".next/**", "!.next/cache/**"]
    }
  },
  "globalDependencies": [
    "tsconfig.json"
  ]
}
